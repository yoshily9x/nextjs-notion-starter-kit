#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/@shuding+opentype.js@1.4.0-beta.0/node_modules/@shuding/opentype.js/bin/node_modules:/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/@shuding+opentype.js@1.4.0-beta.0/node_modules/@shuding/opentype.js/node_modules:/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/@shuding+opentype.js@1.4.0-beta.0/node_modules/@shuding/node_modules:/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/@shuding+opentype.js@1.4.0-beta.0/node_modules:/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/@shuding+opentype.js@1.4.0-beta.0/node_modules/@shuding/opentype.js/bin/node_modules:/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/@shuding+opentype.js@1.4.0-beta.0/node_modules/@shuding/opentype.js/node_modules:/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/@shuding+opentype.js@1.4.0-beta.0/node_modules/@shuding/node_modules:/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/@shuding+opentype.js@1.4.0-beta.0/node_modules:/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@shuding+opentype.js@1.4.0-beta.0/node_modules/@shuding/opentype.js/bin/ot" "$@"
else
  exec node  "$basedir/../../../../../@shuding+opentype.js@1.4.0-beta.0/node_modules/@shuding/opentype.js/bin/ot" "$@"
fi
