#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/prettier@2.8.8/node_modules/prettier/node_modules:/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/prettier@2.8.8/node_modules:/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/prettier@2.8.8/node_modules/prettier/node_modules:/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/prettier@2.8.8/node_modules:/mnt/c/Developments/nextjs-notion-starter-kit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../prettier/bin-prettier.js" "$@"
fi
